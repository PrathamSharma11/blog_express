<%-   include('../header.ejs') %>

<section class="cmspage mtb-40">
    <div class="container">
      <div class="page-desc">
        <div class="row justify-content-center">
          <div class="col-md-8">
            <h1>Contact Display</h1>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>S.No.</th>
                    
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    
                </tr>
                <% i=1 %>
                <% d.forEach((b)=>{ %>
                  <tr>
                    
                    <td><%= i++ %></td>
                    <td><%= b.name %> </td>
                    <td><%= b.email %> </td>
                    <td><%= b.phone %> </td>
                    
                    
                      
                    
                  </tr>
                <%})%>
    
              
                
                
            </table>
          </div>
        </div>
      </div>
    </div>
</section>




<%-   include('../footer.ejs') %>